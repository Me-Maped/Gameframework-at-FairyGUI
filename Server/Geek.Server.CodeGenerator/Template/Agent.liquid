			{{~ else ~}}{{~#************************************************~}}
			(bool needEnqueue, long chainId)= base.Actor.WorkerActor.IsNeedEnqueue();
			{{~ if mth.isasync ~}}
			if (!needEnqueue)
			{
				return await base.{{mth.name}}{{mth.typeparams}}({{mth.paramstr}});
			}
			return await base.Actor.WorkerActor.Enqueue(()=>base.{{mth.name}}{{mth.typeparams}}({{mth.paramstr}}), chainId, {{mth.discard}}, {{mth.timeout}});
			{{~ else ~}}
			if (!needEnqueue)
			{
				return base.{{mth.name}}{{mth.typeparams}}({{mth.paramstr}});
			}
			return base.Actor.WorkerActor.Enqueue(()=>base.{{mth.name}}{{mth.typeparams}}({{mth.paramstr}}), chainId, {{mth.discard}}, {{mth.timeout}});
			{{~ end ~}}
			{{~ end ~}}{{~#************************************************~}}
		}

		{{~ end ~}}
	}
}
