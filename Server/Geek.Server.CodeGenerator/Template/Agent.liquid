			{{~ if mth.discard ~}}{{~#************************************************~}}

			{{~ if mth.threadsafe ~}}
			_ = base.{{mth.name}}{{mth.typeparams}}({{mth.paramstr}});
			{{~ else ~}}
			long callChainId = Geek.Server.Core.Actors.Impl.WorkerActor.NextChainId();
			_ = base.Actor.WorkerActor.Enqueue(()=>base.{{mth.name}}{{mth.typeparams}}({{mth.paramstr}}), callChainId, {{mth.discard}}, {{mth.timeout}});
			{{~ end ~}}
